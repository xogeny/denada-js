  set location = { "type": "string" } "location";
  set model = { "type": "string" } "model";
  set networkName = { "type": "string" } "name?";
}

computer _ "computer*" {
  set location = { "type": "string" } "location";
  set model = { "type": "string" } "model";
  set networkName = { "type": "string" } "name?";
